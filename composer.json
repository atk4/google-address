{
  "name": "atk4/google-address",
  "description": "Google Place interface for address form.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "atk4",
    "agile",
    "agile ui",
    "data",
    "framework"
  ],
  "authors": [
    {
      "name": "Alain Belair",
      "email": "belair.alain@gmail.com"
    }
  ],
  "homepage": "https://github.com/atk4/google-address",
  "require": {
    "php": ">=7.4 <8.3",
    "atk4/ui": "dev-develop"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.13",
    "friendsofphp/php-cs-fixer": "^3.12",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Atk4\\GoogleAddress\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Atk4\\GoogleAddress\\tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  },
  "require-release": {
    "php": ">=7.4 <8.3",
    "atk4/ui": "~4.0.0"
  }
}
